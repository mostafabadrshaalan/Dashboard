@model IdentityRole
@{
    ViewData["Title"] = "Details";
}

<h1>Role Details</h1>
<br />
<br />

<div class="row">
    <div class="col-12">
        <div class="form-group">
             <label asp-for="@Model.Id" class="col-form-label"></Label>
             <input asp-for="@Model.Id" class="form-control" readonly/>
        </div>
        <div class="form-group">
             <label asp-for="@Model.Name" class="col-form-label"></Label>
             <input asp-for="@Model.Name" class="form-control" readonly/>
        </div>

         <br />
         <a class="btn btn-success" asp-action="Update" asp-route-id ="@Model.Id">Edit</a> |
         <a class="btn btn-light" asp-action="Index"> Back To Lists</a>
    </div> 
</div>



